<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="group_library" model="res.groups">
            <field name="name">Library</field>
            <field name="comment">
                Library members have access to all of the library module CRUD operations.
            </field>
        </record>
        <record id="group_client" model="res.groups">
            <field name="name">Client</field>
            <field name="comment">
                Clients only have read access on books.
            </field>
        </record>
    </data>
</odoo>